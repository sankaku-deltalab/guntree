{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "ter-indent": [
      true,
      4
    ],
    "max-line-length": [
      true,
      120
    ],
    "class-name": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format"
    ],
    "prettier": true
  }
}