{
  "name": "guntree",
  "version": "0.9.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "run-s clean build:lib",
    "build:lib": "tsc -p tsconfig.release.json",
    "test": "jest --coverage",
    "prepare": "run-s build",
    "clean": "rimraf ./lib",
    "lint": "eslint --ext .ts src/ test/",
    "indexing": "run-s indexing:create indexing:format",
    "indexing:create": "cti src/contents src/elements",
    "indexing:format": "eslint --ext .ts src/**/index.ts --fix",
    "generate-document": "typedoc --out public/ src/"
  },
  "files": [
    "lib/",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@typescript-eslint/typescript-estree": "^2.8.0",
    "create-ts-index": "^1.12.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.19",
    "transformation-matrix": "^2.2.0"
  },
  "exclude": [
    "node_modules"
  ]
}
